#!/bin/bash
# SPDX-License-Identifier: GPL-2.0

set -e
for f in `ls ${VERSIONING}/*`
  do
    source $f
  done
if [ -f "customize.sh" ] ; then
  source customize.sh
fi
cd Packages

echo "** prepare_revolve **"
set -x

# Make dirs
mkdir -p /opt/scipts/tools/

# Copy files
cp revolve/am335x-revolve.dtb /boot/dtbs/4.14.108-ti-r108/
cp revolve/usb.sh /opt/scripts/tools/eMMC/
cp revolve/functions.sh /opt/scripts/tools/eMMC/
cp revolve/MLO /opt/backup/uboot/MLO
cp revolve/u-boot.img /opt/backup/uboot/u-boot.img

