#!/bin/bash
# SPDX-License-Identifier: GPL-2.0

set -e
for f in `ls ${VERSIONING}/*`
  do
    source $f
  done
if [ -f "customize.sh" ] ; then
  source customize.sh
fi
cd Packages

echo "** Installing usbreset **"
set -x
cd USB
cc usbreset.c -o usbreset
chmod +x usbreset
mv usbreset /usr/local/sbin/
