#!/bin/bash
# SPDX-License-Identifier: GPL-2.0

set -e
for f in `ls ${VERSIONING}/*`
  do
    source $f
  done
cd Packages

echo "** install U-boot**"
set -x
cd ${UBOOT_HOME}

if [ ! -f ${UBOOT_SPL} ] ; then
    wget ${UBOOT_SPL_URL}
fi

if [ ! -f ${UBOOT_BIN} ] ; then
    wget ${UBOOT_BIN_URL}
fi

cp ${UBOOT_SPL} /opt/backup/uboot/MLO
cp ${UBOOT_BIN} /opt/backup/uboot/u-boot.img
